	<script>
		jQuery(document).ready(function() {       
		  
		  $("#FindId").keyup(function(){
			var vals = $('#FindId').val();
			if (vals=='') {
			$('#disdet').html("");
			$('#disrep').html("");
			}		  
		  
		  });

		$('#GetFindRes').click(function () {
		var val = $('#FindId').val();
		$('#icnfnd').hide();
		$('#disfnd').show();
			$('#disrep').html("");
			 $('#disdet').html("");
			                $.ajax({
	                    'dataType': 'json',
	                    'type'    : 'GET',
	                    'url'     : 'http://greenocean.in/n-rest/index.php/api/example/pragoreget',
	                    'data'    : {data: val},
	                    'success' : function(data){
	                        
	                        if (data.response =='true')
	                        {
	                        	
	                 $('#icnfnd').show();
		$('#disfnd').hide();
	                        	 $('#disdet').html("");
	                        	 $('#disdet').html("<table class='table table-bordered  table-hover' id='disresult'>");
	                        	 $('#disresult').append("<tr><th>Name</th><td>"+data.name+"</td></tr><tr><th>Id</th><td>"+data.emp_id+"</td></tr><tr><th>Gender</th><td>"+data.sex+"</td></tr><tr><th>Age</th><td>"+data.age+"</td></tr>");
	                        	 $('#disdet').append("</table>");
	                        	      
     												   sessionStorage['counter'] =data.emp_id;
     												   sessionStorage['pername'] =data.name;
     												   sessionStorage['validationid'] ="1";
     												   
   											 		document.querySelector('#content').innerHTML = sessionStorage.getItem('counter');
   											 		
   											 		
   											 		
	                        }
	                        else {
	                        $('#icnfnd').show();
		$('#disfnd').hide();
	
}
	                    },
	                    'error': function () {
	                    	                 $('#icnfnd').show();
		$('#disfnd').hide();
		$('#GetFindRes').removeClass('green');
		$('#GetFindRes').addClass('red');
	
}
	                });
	                
	                $.ajax({
	                    'dataType': 'json',
	                    'type'    : 'GET',
	                    'url'     : 'http://greenocean.in/n-rest/index.php/api/example/pragoregetres',
	                    'data'    : {data: val},
	                    'success' : function(data){
		                        if (data.response =='true')
	                        {
	                           $('#icnfnd').show();
										$('#disfnd').hide();
										$('#disrep').html("");
										$('#disrep').html("<table class='table table-bordered table-hover' id='RepResult'>");
                         		$('#RepResult').html("<th>Temp</th><th>Spo2</th><th>Pulse</th><th>B.P</th>");
                         		$('#RepResult').append("<tr><td>"+data.temp+"</td><td>"+data.spo2+"</td><td>"+data.pulse+"</td><td>"+data.bp+"</td></tr>");
										$('#disrep').append("</table>");
	                       	}
	                       	else {
	$('#icnfnd').show();
		$('#disfnd').hide();
}
	                    },
	                    'error': function () {
	                    	                 $('#icnfnd').show();
		$('#disfnd').hide();
		$('#GetFindRes').removeClass('green');
		$('#GetFindRes').addClass('red');

	
}
	                });

                
	                
		});
		
		
		

		
		
		   
		});
	</script>	
	
