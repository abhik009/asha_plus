<script type="text/javascript">
	$(function(){//start
	

	$("#temp").click(function (){alert("temp");});
	$("#torchOn").click(function (){alert("torchOn");});
	$("#bpm").click(function (){alert("bpm");});
	$("#pox").click(function (){alert("pox");});
	$("#steth ").click(function (){alert("steth");});
	//$("#glucometer").click(function (){alert("glucometer");});
	$("#discoverBT").click(function (){discoverDevices();});

	$( "#bt-connect" ).click(function( event, ui ) {
		openRfcomm();
			        /*  switch ($( "#bt-connect" ).val())
			          {
			            case "connect" : openRfcomm();break;
			            case "disconnect" : closeRfcomm();break;
			          }*/

//			alert($( "#enableBT" ).val());			
		});	

	$("#glucometer").click(function (){writeRfcomm('g');});
	
	$("#delch").click(function () {
		var x;
		var r=confirm("Delete A Selected Bluetooth Device(s)!");
		if (r==true)
		  {
		  delval();
		  selectval1();
		  }
		else{}
	});
});//end

	function discoverDevices()
	 {
 		$("#discovering").show();
 	  	window.plugins.BluetoothPlugin.discoverDevices( function(devices) {
		$("#discovering").hide();
		$('#bt-devices-select').html('');
 	   	$('#bt-devices-select').append( $( '<option value="null">-select Device From List-</option>' ) );
 	   	for( var i = 0; i < devices.length; i++ )
		 {
 	   		$('#bt-devices-select').append( $( '<option value="' + devices[i].address + '">' + devices[i].name + '</option>' ) );
 	      	 }

	    }, 	function(error) { alert( 'Error: ' + error ); } );
    
  	  }
	function writeRfcomm(data) 
         {
		$('#bt-data-dump').html( '');
		window.plugins.BluetoothPlugin.write( bp_writeSuccess, bp_writeError, g_socketid,data );
	 }

	function bp_writeSuccess( p_data )
	 {
	 	// Continue reading...
	 	window.plugins.BluetoothPlugin.read( bp_readSuccess, bp_readError, g_socketid );
		return;
	}
	
	function bp_writeError( error ) 
	{
		alert( 'Error: ' + error );
	}

	function initializeDB(tx) 
	{
    	//tx.executeSql('DROP TABLE IF EXISTS DEMO');
        tx.executeSql('CREATE TABLE IF NOT EXISTS Bluevalue (dname, daddress)');
        // tx.executeSql('INSERT INTO DEMO2 (dname, daddress) VALUES (1234,"abcd")');
	}
		
	function successCB() 
	{
        //alert("ASHA+ is now loaded!");
        //alert("success!");
   	}
    
	function errorCB(err) {
   	alert("Error processing SQL: " + err.code);
  	}
	
	function insertTB(tx)
	{
	   var devna = $("#bt-devices-select").val();  
	   //var devna = 1;
           var devad = $("#bt-bondeddevices-select").val();
	   tx.executeSql('INSERT INTO Bluevalue (dname, daddress) VALUES ("' +devna+'","'+devad+'")');
	}
	
	function successTB() 
	{
        //alert("ASHA+ is now loaded!");
     	//  alert("Inserted!");
   	}

	function successTBS() 
	{
        //alert("ASHA+ is now loaded!");
        //  alert("Selected!");
   	}
	function selectTB(tx)
	{
	tx.executeSql('SELECT * FROM Bluevalue', [], readSuccess, errorCB);
	//console.log('SELECT * FROM Bluevalue');
	}
	function deleteTV(tx)
	{
	 tx.executeSql('DELETE FROM Bluevalue', [], delval1, errorCB);
         //console.log('SELECT * FROM Bluevalue');
	}

	function delval1()
	{
	 //alert("deleted");	
	}

	function readSuccess(tx, results)
	{
	
	  var len = results.rows.length;
          $('#readSuccess1').html( len + " Devices Available.");
	  //$('#readSuccess').append("<table border>");
	  $('#readSuccess').html("<th>S.No</th><th>Device Type</th><th>Device Address</th><th> </th>");
    	  for (var i=0; i<len; i++)
	    {
         	$('#readSuccess').append("<tr><td>"+i+"</td><td>"+results.rows.item(i).daddress+"</td><td>"+ results.rows.item(i).dname +"</td><td><INPUT type=checkbox name=chks ></td></tr>");
	//   $('#readSuccess').append("Row = " + i + " name = " + results.rows.item(i).dname + " address =  " + results.rows.item(i).daddress);
	
	
       	    }
	}	

	function bp_readSuccess( p_data ) {
	var getVal = new Array();
	getVal = p_data.split(/,|\s|_/);
	var deg = 248;
	//alert(p_data[0]);
	//	$('#bt-data-dump').html('<br><b>'+getVal[0]+'</b>' );	
	switch (getVal[0])
	{
 		case 'M' : $('#measuring-dump').html('<br><b>Measuring... </b>' );
   			   $('#measuring-dump').show();
			   window.plugins.BluetoothPlugin.read( bp_readSuccess, bp_readError, g_socketid );
			   break;
		case 'E' : var errordisplay ='';
			   var eI = 0;
			   var eJ = 0;
			   for (eI=1;eI<getVal.length-1;eI++)
			   {
				errordisplay = errordisplay + getVal[eI] + ' ';
			   }
			   $('#error-dump').html('<br><b>Error: ' + errordisplay + '</b>' );
			   $('#error-dump').show();
			   break;
		case 'C' : $('#bt-data-dump').html('<br><b>' + getVal[1] + ' ' + getVal[2] + '...</b>' );
			   window.plugins.BluetoothPlugin.read( bp_readSuccess, bp_readError, g_socketid );
			   $('#bt-data-dump').show();
			   $('#error-dump').hide();
			   break;
		case 'B' : $("#sys").val(getVal[1]);
			   $("#dia").val(getVal[2]);
			   $('#bpm-dump').show();
			   $('#error-dump').hide();
			   $('#bt-data-dump').hide();
			   break;
		case 'O' : $("#spo2").val(getVal[1]);
			   $("#pulse").val(getVal[2]);
			   $('#spo2-dump').show();
			   $('#error-dump').hide();
			   $('#bt-data-dump').hide();
			   break;
		case 'T' : $("#therm").val(getVal[3]);
			   $('#therm-dump').show();
 			   $('#error-dump').hide();
			   $('#bt-data-dump').hide();//&deg;F
			   break;
		case 'P' : if (headerVal==1)
				{
					headerVal=0;
					window.plugins.BluetoothPlugin.read( bp_readSuccess, bp_readError, g_socketid );
				}
				else
				{
					headerVal = 1;
					$("#gluco").val(getVal[9]);$('#gluco-dump').show();//&deg;F
				}
				break;
		
	}
	
	return;
}

	function bp_readError( error ) 
	{
		alert( 'Error: ' + error );
	}


function openRfcomm() 
{
		if ($( '#bt-devices-select' ).val())
		{
			console.log( 'Discovery connect' );
			window.plugins.BluetoothPlugin.connect(
				function(socketId) {
					g_socketid = socketId;
					console.log( 'Socket-id: ' + g_socketid );
					alert( 'Connected with ' + $( '#bt-devices-select' ).val());
					
				},
				function(error) {
					alert( 'Error: ' + error );
				},
				$( '#bt-devices-select' ).val(),
				"00001101-0000-1000-8000-00805f9b34fb"
			);
		}
	else
	{
		window.plugins.BluetoothPlugin.connect(
			function(socketId) {
				g_socketid = socketId;
				console.log( 'Socket-id: ' + g_socketid );
				alert( 'Connected with ' + $( '#bt-bondeddevices-select' ).val());
				},
			function(error) {
				alert( 'Error: ' + error );
			},
			$( '#bt-bondeddevices-select' ).val(),
			"00001101-0000-1000-8000-00805f9b34fb"
		);
	}
}

function closeRfcomm()
{
	window.plugins.BluetoothPlugin.disconnect(
		function() 
		{
			alert( 'Disconnected');
		},
		function(error) 
		{
			alert( 'Error: ' + error );
		},
		g_socketid );
}

	
</script>
	

